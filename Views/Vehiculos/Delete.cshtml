@model ConcesionarioWeb.Models.Vehiculo
@{
    ViewData["Title"] = "Eliminar";
}
<h1>Eliminar</h1>
<hr />
<h3>¿Seguro que deseas eliminar este registro?</h3>
<dl>
    <dt>Id</dt><dd>@Model.Id</dd>
    <dt>Marca</dt><dd>@Model.Marca</dd>
    <dt>Modelo</dt><dd>@Model.Modelo</dd>
    <dt>Matrícula</dt><dd>@Model.Matricula</dd>
    <dt>Año</dt><dd>@Model.Año</dd>
    <dt>Tipo de Vehículo</dt><dd>@Model.TipoVehiculo.Nombre</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <button type="submit">Eliminar</button>
    <a asp-action="Index">Cancelar</a>
</form>